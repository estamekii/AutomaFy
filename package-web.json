{
  "name": "automafy-web",
  "version": "1.0.1",
  "description": "AutomaFy Web - Application Installer for VPS",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "install-web": "npm install --production",
