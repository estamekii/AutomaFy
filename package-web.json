{
  "name": "lookstall-web",
  "version": "1.0.1",
  "description": "Lookstall Web - Application Installer for VPS",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "install-web": "npm install --production",
    "pm2-start": "pm2 start server.js --name lookstall-web",
    "pm2-stop": "pm2 stop lookstall-web",
    "pm2-restart": "pm2 restart lookstall-web",
    "pm2-logs": "pm2 logs lookstall-web"
  },
  "keywords": [
    "docker",
    "installer",
    "vps",
    "web",
    "automation",
    "applications"
  ],
